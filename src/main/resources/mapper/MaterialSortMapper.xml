<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dazhao.pojo.mapper.MaterialSortMapper">
  <select id="isUseMaterialSort" resultType="int">
      SELECT COUNT(1)
      FROM material
      WHERE sort_id = #{value}
  </select>

</mapper>